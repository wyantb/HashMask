{
	"name": "Hash Mask",
	"version": "1.0",
	"description": "Secure password visualization for Chrome.",
	"icons": ,
	"default_locale": "en",
	"browser_action": {
		"default_icon": "icon.png",
		"popup": "popup.html"
	},
	"options_page": "options.html",
	"content_scripts": [
		{
		"matches": ["http://*/*", "https://*/*],
		"css": ["inject.css"],
		"js": ["lib/jquery-1.7.1.js", "inject.js"],
		"all_frames": true
		}
	],
	"permissions": [
		"http://example.com"
	]
}
